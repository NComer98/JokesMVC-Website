@model IEnumerable<JokesWebAppMVC.Models.Joke>

@{
    ViewData["Title"] = "Home Page";
}
<body>
    <div class="text-center">
        <h1 class="display-4">Welcome</h1>
        <p>A database of <s>bad</s> <b>EXCELLENT</b> jokes that you can add to.</p>
    </div>

    <div class="text-center">
        <h1>Dynamic Search Powered By ReactJS</h1>
    </div>
    @Html.React("JokesTable", new
    {
        initialData = Model,
        url = "/jokesjson",
        pollInterval = 200000,
    })

    <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react/16.13.0/umd/react.development.js"></script>
    <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.13.0/umd/react-dom.development.js"></script>
    <script src="@Url.Content("~/js/app.jsx")"></script>
    @Html.ReactInitJavaScript()
</body>